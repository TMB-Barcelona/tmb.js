  },
  "homepage": "https://github.com/geomatico/tmb.js#readme",
  "devDependencies": {
    "file-exists": "^1.0.0",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "jasmine-node": "^1.14.5",
    "jsonfile": "^2.2.3",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "prompt": "^1.0.0"
  },
  "dependencies": {
    "axios": "^0.9.1"
