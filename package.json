{
  "name": "tmb.js",
  "version": "0.0.1",
  "description": "TMB javascript library",
  "main": "dist/tmb.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "npm install && node tools/ensure_api_keys.js && webpack",
    "pretest": "npm run build",
    "test": "npm run test:node && npm run test:browsers",
    "test:node": "jasmine-node spec",
    "test:browsers": "karma start karma.conf.js",
    "prestart": "npm run build",
    "start": "http-server -a localhost -p 8491 -c-1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geomatico/tmb.js.git"
  },
  "author": "Oscar Fonts; Micho Garcia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geomatico/tmb.js/issues"
  },
  "homepage": "https://github.com/geomatico/tmb.js#readme",
  "devDependencies": {
    "file-exists": "^1.0.0",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "jasmine-node": "^1.14.5",
    "jsonfile": "^2.2.3",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-read-json": "^1.1.0",
    "phantomjs-prebuilt": "^2.1.7",
    "prompt": "^1.0.0",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "es6-promise": "^3.1.2"
  }
}
