{
  "name": "tmb.js",
  "version": "0.0.1",
  "description": "TMB javascript library",
  "main": "src/tmb.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "setup": "npm install && node tools/ensure_api_keys.js",
